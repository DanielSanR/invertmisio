{
  "api": {
    "baseUrl": "https://api.invertrack.com",
    "timeout": 30000
  },
  "firebase": {
    "apiKey": "YOUR_FIREBASE_API_KEY",
    "authDomain": "invertrack.firebaseapp.com",
    "projectId": "invertrack",
    "storageBucket": "invertrack.appspot.com",
    "messagingSenderId": "YOUR_MESSAGING_SENDER_ID",
    "appId": "YOUR_APP_ID",
    "measurementId": "YOUR_MEASUREMENT_ID"
  },
  "storage": {
    "imageCompression": {
      "maxWidth": 1280,
      "maxHeight": 1280,
      "quality": 0.8
    },
    "maxFileSize": 10485760
  },
  "sync": {
    "interval": 300000,
    "retryAttempts": 3,
    "retryDelay": 5000
  },
  "notifications": {
    "defaultChannel": "default",
    "importance": "high",
    "reminders": {
      "defaultLeadTime": 86400000
    }
  },
  "maps": {
    "defaultRegion": {
      "latitude": -27.3667,
      "longitude": -55.8969,
      "latitudeDelta": 0.5,
      "longitudeDelta": 0.5
    },
    "clustering": {
      "enabled": true,
      "radius": 50
    }
  },
  "export": {
    "defaultFormat": "pdf",
    "maxItems": 1000
  },
  "logging": {
    "level": "error",
    "remote": true
  }
}
